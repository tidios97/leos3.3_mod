    <numberingConfigs>
        <numberingConfig>
            <type>HIGHER_ELEMENT_NUM</type>
            <numbered>true</numbered>
            <prefix></prefix>
            <suffix></suffix>
            <sequence>1</sequence>
            <description>1, 2, etc</description>
            <regex>#|^(?!([a-z]))((^([1-9][0-9]*)|M{0,4}(CM|CD|D?C{0,3})(XC|XL|L?X{0,3})(IX|IV|V?I{0,3}))([a-z])*)</regex>
            <msgValidationError>subdivision.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ARABIC</type>
            <numbered>true</numbered>
            <prefix></prefix>
            <suffix></suffix>
            <sequence>1</sequence>
            <description>1, 2, etc</description>
            <regex>\d+$|#</regex>
            <msgValidationError>arabic.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ARABIC_POSTFIXDOT</type>
            <numbered>true</numbered>
            <prefix></prefix>
            <suffix>.</suffix>
            <sequence>1</sequence>
            <description>1., 2., etc</description>
            <regex>\d+$|#</regex>
            <msgValidationError>arabic.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ARABIC_PARENTHESIS</type>
            <numbered>true</numbered>
            <prefix>(</prefix>
            <suffix>)</suffix>
            <sequence>1</sequence>
            <description>(1), (2), etc</description>
            <regex>\d+$|#</regex>
            <msgValidationError>arabic.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ALPHA_LOWER_PARENTHESIS</type>
            <numbered>true</numbered>
            <prefix>(</prefix>
            <suffix>)</suffix>
            <sequence>a</sequence>
            <description>(a), (b), etc</description>
            <regex>[a-z]+$|#</regex>
        </numberingConfig>
        <numberingConfig>
            <type>ROMAN_LOWER_PARENTHESIS</type>
            <numbered>true</numbered>
            <prefix>(</prefix>
            <suffix>)</suffix>
            <sequence>i</sequence>
            <description>(i), (ii), etc</description>
            <regex>m{0,4}(cm|cd|d?c{0,3})(xc|xl|l?x{0,3})(ix|iv|v?i{0,3})$|#</regex>
            <msgValidationError>roman.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>ROMAN_UPPER</type>
            <numbered>true</numbered>
            <prefix></prefix>
            <suffix></suffix>
            <sequence>I</sequence>
            <description>I, II, etc</description>
            <regex>M{0,4}(CM|CD|D?C{0,3})(XC|XL|L?X{0,3})(IX|IV|V?I{0,3})$|#</regex>
            <msgValidationError>roman.num.config.validation.error.message</msgValidationError>
        </numberingConfig>
        <numberingConfig>
            <type>INDENT</type>
            <numbered>false</numbered>
            <prefix></prefix>
            <suffix></suffix>
            <sequence>-</sequence>
            <description>-, -, etc for all points</description>
        </numberingConfig>

        <numberingConfig>
            <type>NONE</type>
            <numbered>false</numbered>
        </numberingConfig>

        <numberingConfig>
            <type>POINT_NUM</type>
            <numbered>true</numbered>
            <levels>
                <level>
                    <depth>1</depth>
                    <numberingType>ALPHA_LOWER_PARENTHESIS</numberingType>
                </level>
                <level>
                    <depth>2</depth>
                    <numberingType>ROMAN_LOWER_PARENTHESIS</numberingType>
                </level>
                <level>
                    <depth>3</depth>
                    <numberingType>ARABIC_PARENTHESIS</numberingType>
                </level>
                <level>
                    <depth>4</depth>
                    <numberingType>INDENT</numberingType>
                </level>
            </levels>
        </numberingConfig>

        <numberingConfig>
            <type>POINT_NUM_DEF</type>
            <numbered>true</numbered>
            <levels>
                <level>
                    <depth>1</depth>
                    <numberingType>ARABIC_PARENTHESIS</numberingType>
                </level>
                <level>
                    <depth>2</depth>
                    <numberingType>ALPHA_LOWER_PARENTHESIS</numberingType>
                </level>
                <level>
                    <depth>3</depth>
                    <numberingType>ROMAN_LOWER_PARENTHESIS</numberingType>
                </level>
                <level>
                    <depth>4</depth>
                    <numberingType>INDENT</numberingType>
                </level>
            </levels>
        </numberingConfig>
    </numberingConfigs>

