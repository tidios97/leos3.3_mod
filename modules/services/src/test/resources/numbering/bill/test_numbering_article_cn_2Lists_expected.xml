									<alinea leos:origin="ec" xml:id="akn_article_ebFhaW_aCPDNb">
										<content leos:origin="ec" xml:id="akn_article_ebFhaZ_xRZkW7">
											<p leos:origin="ec" xml:id="akn_article_ebFhaZ_kSgpRW">First indent</p>
										</content>
									</alinea>
									<list leos:origin="ec" xml:id="_lis_xRd9Lg">
