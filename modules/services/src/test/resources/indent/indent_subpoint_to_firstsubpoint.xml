<akomaNtoso xmlns="http://docs.oasis-open.org/legaldocml/ns/akn/3.0" xmlns:leos="urn:eu:europa:ec:leos">
    <bill name="REG">
        <body leos:origin="ec" xml:id="body">
            <article leos:autonumbering="true" leos:deletable="false" leos:editable="false" leos:origin="ec" xml:id="art_1">
                <num leos:editable="false" leos:origin="ec" xml:id="art_1__num">Article 1</num>
                <heading leos:origin="ec" xml:id="art_1__heading">Scope</heading>
                <paragraph leos:origin="ec" xml:id="art_1_RXdbC0">
                    <num leos:origin="ec" xml:id="art_1_uXQ0yL">1.</num>
                    <content leos:origin="ec" xml:id="art_1_wmf7Mf">
                        <p leos:origin="ec" xml:id="art_1_mpZ0zS">POINT</p>
                    </content>
                </paragraph>
                <paragraph leos:origin="ec" xml:id="art_1_GqEOBU">
                    <num leos:origin="ec" xml:id="art_1_Woc735">2.</num>
                    <subparagraph leos:origin="ec" xml:id="art_1_MU1hKr">
                        <content leos:origin="ec" xml:id="art_1_fCeC1t">
                            <p leos:origin="ec" xml:id="art_1_NT9OhJ">ExampleÂ --
                                <b leos:origin="ec" xml:id="art_1_Klz48x">Point (c) indented</b>
                            </p>
                        </content>
                    </subparagraph>
                    <list leos:origin="ec" xml:id="art_1_Wnl7op">
                        <point leos:origin="ec" xml:id="art_1_urNCcV">
                            <num leos:origin="ec" xml:id="art_1_gFkBde">(a)</num>
                            <content leos:origin="ec" xml:id="art_1_Qzmsjc">
                                <p leos:origin="ec" xml:id="art_1_aQ7g6K">test</p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="art_1_kWNiUC">
                            <num leos:origin="ec" xml:id="art_1_acDbga">(b)</num>
                            <content leos:origin="ec" xml:id="art_1_flq9C1">
                                <p leos:origin="ec" xml:id="art_1_kw4sXt">'introduction of cultural goods' means any entry into the customs territory of the Union of cultural goods which are subject to customs supervision or customs control within the customs territory of the Union in accordance with Regulation (EU) No 952/2013;</p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="art_1_ZzykdM">
                            <num leos:origin="ec" xml:id="art_1_koxWzM">(c)</num>
                            <alinea leos:origin="ec" xml:id="art_1_78ndzv">
                                <content leos:origin="ec" xml:id="art_1_EzAUvo">
                                    <p leos:origin="ec" xml:id="art_1_17jvrP">'import of cultural goods' means:</p>
                                </content>
                            </alinea>
                            <list leos:origin="ec" xml:id="art_1_bQW8O7">
                                <point leos:origin="ec" xml:id="art_1_azxVUY">
                                    <num leos:origin="ec" xml:id="art_1_8G6jeY">(i)</num>
                                    <content leos:origin="ec" xml:id="art_1_yoa6NC">
                                        <p leos:origin="ec" xml:id="art_1_0mo2Fc">the release of cultural goods for free circulation as referred to in Article 201 of Regulation (EU) No 952/2013; or</p>
                                    </content>
                                </point>
                                <point leos:origin="ec" xml:id="art_1_mSvIgb">
                                    <num leos:origin="ec" xml:id="art_1_Fyvr9w">(ii)</num>
                                    <alinea leos:origin="ec" xml:id="art_1_6G73F7">
                                        <content leos:origin="ec" xml:id="art_1_keYk7S">
                                            <p leos:origin="ec" xml:id="art_1_PMAuPF">the placing of cultural goods under one of the following categories of special procedures referred to in Article 210 of Regulation (EU) No 952/2013:</p>
                                        </content>
                                    </alinea>
                                    <list leos:origin="ec" xml:id="art_1_gDJwrD">
                                        <point leos:origin="ec" xml:id="art_1_1juLXq">
                                            <num leos:origin="ec" xml:id="art_1_Wxy2we">(1)</num>
                                            <content leos:origin="ec" xml:id="art_1_N0JGOv">
                                                <p leos:origin="ec" xml:id="art_1_ddsnGD">storage, comprising customs warehousing and free zones;</p>
                                            </content>
                                        </point>
                                        <point leos:origin="ec" xml:id="art_1_SQHh5n">
                                            <num leos:origin="ec" xml:id="art_1_Z61Xsc">(2)</num>
                                            <alinea leos:origin="ec" xml:id="art_1_6G45GHJ">
                                                <content leos:origin="ec" xml:id="art_1_SPx97b">
                                                    <p leos:origin="ec" xml:id="art_1_iALt29">specific use, comprising temporary admission and end-use;</p>
                                                </content>
                                            </alinea>
                                            <list leos:origin="ec" xml:id="art_1_gDGhFRT">
                                                <indent leos:indent-origin-indent-level="4" leos:indent-origin-num-origin="ec" leos:indent-origin-type="FIRST_SUBPOINT" leos:origin="ec" leos:softaction="trans" leos:softactionroot="true" leos:softdate="2021-11-03T18:10:14.088+01:00" leos:softtrans_from="art_1_6G46GHJ" leos:softuser="demo" xml:id="art_1_1juLJh">
                                                    <num leos:origin="ec" xml:id="art_1_Wxy2we_1">-</num>
                                                    <content leos:origin="ec" xml:id="art_1_N0JGOv_1">
                                                        <p leos:origin="ec" xml:id="art_1_ddsnGD_1">example of indent with subpoint</p>
                                                    </content>
                                                </indent>
                                            </list>
                                        </point>
                                    </list>
                                </point>
                                <point leos:indent-origin-indent-level="5" leos:indent-origin-type="OTHER_SUBPOINT" leos:origin="ec" leos:softaction="trans" leos:softactionroot="true" leos:softdate="2021-11-03T18:10:14.089+01:00" leos:softuser="demo" xml:id="transformed_art_1_6G47GHJ">
                                    <num class="leos-content-soft-new" leos:origin="cn" xml:id="num_OaXM9N">
                                        <span class="leos-content-soft-new">(iia)</span>
                                    </num>
                                    <alinea leos:origin="ec" xml:id="art_1_6G47GHJ">
                                        <content leos:origin="ec" xml:id="art_1_NGHSZ">
                                            <p leos:origin="ec" xml:id="art_1_bgH78">subpoint of indent</p>
                                        </content>
                                    </alinea>
                                    <list class="leos-content-soft-new" leos:origin="ec" xml:id="lis_zftkvS">
                                        <point leos:origin="ec" xml:id="art_1_kqZEk9">
                                            <num leos:origin="ec" xml:id="art_1_vqCBfZ">(3)</num>
                                            <content leos:origin="ec" xml:id="art_1_2RKX4f">
                                                <p leos:origin="ec" xml:id="art_1_jX0q3V">inward processing;</p>
                                            </content>
                                        </point>
                                    </list>
                                </point>
                                <point leos:origin="ec" xml:id="art_1_ZbWmIy">
                                    <num leos:origin="ec" xml:id="art_1_ZUKjt1">(iii)</num>
                                    <content leos:origin="ec" xml:id="art_1_OsSaib">
                                        <p leos:origin="ec" xml:id="art_1_dPvXRD">'holder of the goods' means holder of the goods as defined in point (34) of Article 5 of Regulation (EU) No 952/2013;</p>
                                    </content>
                                </point>
                            </list>
                        </point>
                    </list>
                </paragraph>
            </article>
            <clause leos:alternative="true" leos:deletable="true" leos:editable="true" leos:optionlist="listOption1" leos:origin="ec" leos:selectedoption="1" xml:id="clause_1">
                <content leos:origin="ec" xml:id="clause_1__content">
                    <p leos:origin="ec" xml:id="clause_1__content__p">This Regulation shall be binding in its entirety and directly applicable in all Member States.</p>
                </content>
            </clause>
        </body>
