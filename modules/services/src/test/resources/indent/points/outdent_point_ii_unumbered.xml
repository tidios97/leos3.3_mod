            <article leos:autonumbering="true" leos:deletable="false" leos:editable="false" leos:origin="ec"
                     xml:id="art_1">
                <num leos:editable="false" leos:origin="ec" xml:id="art_1__num">Article 1</num>
                <heading leos:origin="ec" xml:id="art_1__heading">Scope</heading>
                <paragraph leos:origin="ec" xml:id="art_1_RXdbC0">
                    <num leos:origin="ec" xml:id="art_1_uXQ0yL">1.</num>
                    <content leos:origin="ec" xml:id="art_1_wmf7Mf">
                        <p leos:origin="ec" xml:id="art_1_mpZ0zS">POINT</p>
                    </content>
                </paragraph>
                <paragraph leos:origin="ec" xml:id="art_1_GqEOBU">
                    <num leos:origin="ec" xml:id="art_1_Woc735">2.</num>
                    <subparagraph leos:origin="ec" xml:id="art_1_MU1hKr">
                        <content leos:origin="ec" xml:id="art_1_fCeC1t">
                            <p leos:origin="ec" xml:id="art_1_NT9OhJ">ExampleÂ --
                                <b leos:origin="ec" xml:id="art_1_Klz48x">Point (c) indented</b>
                            </p>
                        </content>
                    </subparagraph>
                    <list leos:origin="ec" xml:id="art_1_Wnl7op">
                        <point leos:origin="ec" xml:id="art_1_urNCcV">
                            <num leos:origin="ec" xml:id="art_1_gFkBde">(a)</num>
                            <content leos:origin="ec" xml:id="art_1_Qzmsjc">
                                <p leos:origin="ec" xml:id="art_1_aQ7g6K">test</p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="art_1_kWNiUC">
                            <num leos:origin="ec" xml:id="art_1_acDbga">(b)</num>
                            <content leos:origin="ec" xml:id="art_1_flq9C1">
                                <p leos:origin="ec" xml:id="art_1_kw4sXt">'introduction of cultural goods' means any
                                    <span class="leos-content-soft-removed"></span>
                                    <span class="leos-content-soft-new"></span>entry into the customs territory of the
                                    Union of cultural goods which are subject to
                                    <span class="leos-content-soft-removed"></span>
                                    <span class="leos-content-soft-new"></span>customs supervision or customs control
                                    within the customs territory of the Union in
                                    <span class="leos-content-soft-removed"></span>
                                    <span class="leos-content-soft-new"></span>accordance with Regulation (EU) No
                                    952/2013;
                                    <span class="leos-content-soft-removed"></span>
                                </p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="art_1_ZzykdM">
                            <num leos:origin="ec" xml:id="art_1_koxWzM">(c)</num>
                            <alinea leos:origin="ec" xml:id="art_1_78ndzv">
                                <content leos:origin="ec" xml:id="art_1_EzAUvo">
                                    <p leos:origin="ec" xml:id="art_1_17jvrP">'import of cultural goods' means:</p>
                                </content>
                            </alinea>
                            <list leos:origin="ec" xml:id="art_1_bQW8O7">
                                <point leos:origin="ec" xml:id="art_1_azxVUY">
                                    <num leos:origin="ec" xml:id="art_1_8G6jeY">(i)</num>
                                    <content leos:origin="ec" xml:id="art_1_yoa6NC">
                                        <p leos:origin="ec" xml:id="art_1_0mo2Fc">the release of cultural goods for free
                                            <span class="leos-content-soft-removed"></span>
                                            <span class="leos-content-soft-new"></span>circulation as referred to in
                                            Article 201 of Regulation (EU) No 952/2013; or
                                            <span class="leos-content-soft-removed"></span>
                                        </p>
                                    </content>
                                </point>
                            </list>
                            <alinea leos:indent-origin-indent-level="2" leos:indent-origin-num="(ii)"
                                    leos:indent-origin-num-id="art_1_Fyvr9w" leos:indent-origin-num-origin="ec"
                                    leos:indent-origin-type="FIRST_SUBPOINT" leos:origin="ec" leos:softaction="trans"
                                    leos:softactionroot="true" leos:softdate="2022-06-17T00:41:41.763+02:00"
                                    leos:softtrans_from="art_1_6G73F7" leos:softuser="demo(DGT)" xml:id="art_1_mSvIgb">
                                <content leos:origin="ec" xml:id="art_1_keYk7S">
                                    <p leos:origin="ec" xml:id="art_1_PMAuPF">the placing of cultural goods
                                        <span class="leos-content-soft-removed"></span>
                                        <span class="leos-content-soft-new"></span>under one of the following categories
                                        of special procedures referred to
                                        <span class="leos-content-soft-removed"></span>
                                        <span class="leos-content-soft-new"></span>in Article 210 of Regulation (EU) No
                                        952/2013:
                                        <span class="leos-content-soft-removed"></span>
                                    </p>
                                </content>
                            </alinea>
                            <list class="leos-content-soft-new" leos:origin="ec" xml:id="lis_sXXVjA">
                                <point leos:indent-origin-indent-level="3" leos:indent-origin-num="(1)"
                                       leos:indent-origin-num-id="art_1_Wxy2we" leos:indent-origin-num-origin="ec"
                                       leos:indent-origin-type="POINT" leos:origin="ec" leos:softaction="trans"
                                       leos:softactionroot="true" leos:softdate="2022-06-17T00:41:41.778+02:00"
                                       leos:softuser="demo(DGT)" xml:id="transformed_art_1_1juLXq">
                                    <num leos:origin="cn" xml:id="art_1_Wxy2we">
                                        <span class="leos-content-soft-removed">(1)</span>
                                        <span class="leos-content-soft-new">(-iii)</span>
                                    </num>
                                    <alinea leos:origin="ec" xml:id="art_1_1juLXq">
                                        <content leos:origin="ec" xml:id="art_1_N0JGOv">
                                            <p leos:origin="ec" xml:id="art_1_ddsnGD">storage, comprising customs
                                                <span class="leos-content-soft-removed"></span>
                                                <span class="leos-content-soft-new"></span>warehousing and free zones;
                                                <span class="leos-content-soft-removed"></span>
                                            </p>
                                        </content>
                                    </alinea>
                                    <alinea leos:indent-origin-indent-level="3" leos:indent-origin-num="(2)"
                                            leos:indent-origin-num-id="art_1_Z61Xsc" leos:indent-origin-num-origin="ec"
                                            leos:indent-origin-type="POINT" leos:origin="ec" leos:softaction="trans"
                                            leos:softactionroot="true" leos:softdate="2021-05-04T11:09:12.000+02:00"
                                            leos:softuser="jane" xml:id="art_1_SQHh5n">
                                        <content leos:origin="ec" xml:id="art_1_SPx97b">
                                            <p leos:origin="ec" xml:id="art_1_iALt29">specific use, comprising
                                                <span class="leos-content-soft-removed"></span>
                                                <span class="leos-content-soft-new"></span>temporary admission and
                                                end-use;
                                                <span class="leos-content-soft-removed"></span>
                                            </p>
                                        </content>
                                    </alinea>
                                    <list class="leos-content-soft-new" leos:origin="ec" xml:id="art_1_gDJwrD">
                                        <point class="leos-content-soft-new" leos:indent-origin-indent-level="4"
                                               leos:indent-origin-num="-" leos:indent-origin-num-id="art_1_Jc9SO7"
                                               leos:indent-origin-num-origin="cn" leos:indent-origin-type="POINT"
                                               leos:origin="cn" leos:softaction="add"
                                               leos:softdate="2021-05-04T11:09:12.000+02:00" leos:softuser="jane"
                                               xml:id="akn_point_Q1c5WA">
                                            <num class="leos-content-soft-new" leos:origin="cn" xml:id="art_1_Jc9SO7">
                                                (-3)
                                            </num>
                                            <content class="leos-content-soft-new" xml:id="art_1_eKCme7">
                                                <p class="leos-content-soft-new" xml:id="art_1_erx2LB">
                                                    <span class="leos-content-soft-new">Text...</span>
                                                </p>
                                            </content>
                                        </point>
                                        <point leos:origin="ec" xml:id="art_1_kqZEk9">
                                            <num leos:origin="ec" xml:id="art_1_vqCBfZ">(3)</num>
                                            <content leos:origin="ec" xml:id="art_1_2RKX4f">
                                                <p leos:origin="ec" xml:id="art_1_jX0q3V">inward processing;</p>
                                            </content>
                                        </point>
                                    </list>
                                </point>
                                <point leos:origin="ec" xml:id="art_1_ZbWmIy">
                                    <num leos:origin="ec" xml:id="art_1_ZUKjt1">(iii)</num>
                                    <content leos:origin="ec" xml:id="art_1_OsSaib">
                                        <p leos:origin="ec" xml:id="art_1_dPvXRD">'holder of the goods' means holder of
                                            <span class="leos-content-soft-removed"></span>
                                            <span class="leos-content-soft-new"></span>the goods as defined in point
                                            (34) of Article 5 of Regulation (EU)
                                            <span class="leos-content-soft-removed">No 952/2013</span>
                                            <span class="leos-content-soft-new">No 952/2013</span>;
                                            <span class="leos-content-soft-removed"></span>
                                        </p>
                                    </content>
                                </point>
                            </list>
                        </point>
                    </list>
                </paragraph>
            </article>
            <clause leos:alternative="true" leos:deletable="true" leos:editable="true" leos:optionlist="listOption1"
                    leos:origin="ec" leos:selectedoption="1" xml:id="clause_1">
                <content leos:origin="ec" xml:id="clause_1__content">
                    <p leos:origin="ec" xml:id="clause_1__content__p">This Regulation shall be binding in its entirety
                        <span class="leos-content-soft-removed"></span>
                        <span class="leos-content-soft-new"></span>and directly applicable in all Member States.
                        <span class="leos-content-soft-removed"></span>
                    </p>
                </content>
            </clause>
