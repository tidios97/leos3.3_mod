<akomaNtoso xmlns="http://docs.oasis-open.org/legaldocml/ns/akn/3.0" xmlns:leos="urn:eu:europa:ec:leos">
    <bill name="REG">
        <body leos:origin="ec" xml:id="body">
            <article leos:deletable="false" leos:editable="true" leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr">
                <num leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_ycIXvt">Article 1</num>
                <heading leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_vXomtu">Definitions</heading>
                <paragraph leos:indent-origin-indent-level="0" leos:indent-origin-type="FIRST_SUBPARAGRAPH"
                           leos:indent-unumbered-paragraph="true" leos:origin="ec" leos:softaction="trans"
                           leos:softactionroot="true" leos:softdate="2022-06-16T18:54:08.550+02:00"
                           leos:softtrans_from="imp_art_d1e730_u0rbKr_SiJne0" leos:softuser="demo(DGT)"
                           xml:id="imp_art_d1e730_u0rbKr_WmW4bL">
                    <content leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_2kSVKO">
                        <p leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_Izfl3x">For the purposes of this Directive,
                            the following definitions apply:
                        </p>
                    </content>
                </paragraph>
                <paragraph leos:indent-origin-indent-level="1" leos:indent-origin-num="(a)"
                           leos:indent-origin-num-id="imp_art_d1e730_u0rbKr_12A98c" leos:indent-origin-num-origin="ec"
                           leos:indent-origin-type="POINT" leos:indent-unumbered-paragraph="true" leos:origin="ec"
                           leos:softaction="trans" leos:softactionroot="true"
                           leos:softdate="2022-06-16T18:54:08.566+02:00" leos:softuser="demo(DGT)"
                           xml:id="transformed_imp_art_d1e730_u0rbKr_V6IrA4">
                    <subparagraph leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_V6IrA4">
                        <content leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_XZX3px">
                            <p leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_qgrjFd">'public sector body' means the
                                State, regional or local authorities, bodies governed by public law, as defined in point
                                (4) of Article 2(1) of Directive 2014/24/EU, or associations formed by one or more such
                                authorities or one or more such bodies governed by public law, if those associations are
                                established for the specific purpose of meeting needs in the general interest, not
                                having an industrial or commercial character;
                            </p>
                        </content>
                    </subparagraph>
                    <list class="leos-content-soft-new" leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_i7uOSY">
                        <point leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_0i44Nc">
                            <num leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_qVpLAK">(b)</num>
                            <content leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_XebqeZ">
                                <p leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_WqENUJ">'mobile application' means
                                    application software designed and developed, by or on behalf of public sector
                                    bodies, for use by the general public on mobile devices such as smartphones and
                                    tablets. It does not include the software that controls those devices (mobile
                                    operating systems) or hardware;
                                </p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_zEw0cz">
                            <num leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_kiKZAf">(c)</num>
                            <content leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_sND0vi">
                                <p leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_7OQ6Ge">'standard' means a standard as
                                    defined in point (1) of Article 2 of Regulation (EU) No 1025/2012;
                                </p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_Nbh7ZL">
                            <num leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_91owTD">(d)</num>
                            <content leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_JhwafC">
                                <p leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_URfN4w">'European standard' means a
                                    European standard as defined in point (1)(b) of Article 2 of Regulation (EU) No
                                    1025/2012;
                                </p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_dwUXOZ">
                            <num leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_IYySd7">(e)</num>
                            <content leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_dNfSQ6">
                                <p leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_06YFhC">'harmonised standard' means a
                                    harmonised standard as defined in point (1)(c) of Article 2 of Regulation (EU) No
                                    1025/2012;
                                </p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_bsMgKx">
                            <num leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_GGIvvs">(f)</num>
                            <content leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_hdSy5R">
                                <p leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_ClDrCM">'time-based media' means media
                                    of the following types: audio-only, video-only, audio-video, audio and/or video
                                    combined with interaction;
                                </p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_HMVrAc">
                            <num leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_1fz6aA">(g)</num>
                            <content leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_MHoTkb">
                                <p leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_AIX9sv">'items in heritage
                                    collections' means privately or publicly owned goods presenting an historical,
                                    artistic, archaeological, aesthetic, scientific or technical interest and that are
                                    part of collections preserved by cultural institutions such as libraries, archives
                                    and museums;
                                </p>
                            </content>
                        </point>
                        <point leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_FsO1Vo">
                            <num leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_cyWBRU">(h)</num>
                            <content leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_5VtGsF">
                                <p leos:origin="ec" xml:id="imp_art_d1e730_u0rbKr_i5D7qk">'measurement data' means the
                                    quantified results of the monitoring activity carried out in order to verify the
                                    compliance of the websites and mobile applications of public sector bodies with the
                                    accessibility requirements set out in Article 4. It covers both quantitative
                                    information about the sample of websites and mobile applications tested (number of
                                    websites and applications with, potentially, the number of visitors or users, etc.)
                                    and quantitative information about the level of accessibility.
                                </p>
                            </content>
                        </point>
                    </list>
                </paragraph>
            </article>
        </body>
    </bill>
</akomaNtoso>