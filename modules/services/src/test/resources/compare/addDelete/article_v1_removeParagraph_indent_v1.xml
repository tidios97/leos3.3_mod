													<content xml:id="_akn_article_ebFhaW_kzrYdh">
														<p xml:id="_akn_article_ebFhaW_qsoGmP">indent</p>
													</content>
												</indent>
											</list>
										</point>
									</list>
								</point>
							</list>
						</point>
						<point leos:indent-origin-indent-level="4" leos:indent-origin-num="-" leos:indent-origin-num-id="akn_article_ebFhaW_HMyEvN" leos:indent-origin-num-origin="ec" leos:indent-origin-type="POINT" leos:origin="ec" leos:softaction="trans" leos:softactionroot="true" leos:softdate="2021-12-09T15:49:38.692+01:00" leos:softuser="jane" xml:id="transformed_akn_article_ebFhaW_9GxqxT">
							<num leos:origin="cn" xml:id="akn_article_ebFhaW_HMyEvN">(ca)</num>
							<alinea leos:origin="ec" xml:id="akn_article_ebFhaW_9GxqxT">
								<content leos:origin="ec" xml:id="akn_article_ebFhaW_DY3cSH">
									<p leos:origin="ec" xml:id="akn_article_ebFhaW_R8CE0A">Third indent</p>
								</content>
							</alinea>
							<list leos:origin="ec" xml:id="_lis_HqSVXm">
								<point leos:indent-origin-indent-level="3" leos:indent-origin-num="(3)" leos:indent-origin-num-id="akn_article_ebFhaW_XRXtjV" leos:indent-origin-num-origin="ec" leos:indent-origin-type="POINT" leos:origin="ec" leos:softaction="trans" leos:softactionroot="true" leos:softdate="2021-12-09T15:49:38.692+01:00" leos:softuser="jane" xml:id="transformed_akn_article_ebFhaW_gX0mG3">
									<num leos:origin="cn" xml:id="akn_article_ebFhaW_XRXtjV">(-iii)</num>
									<alinea leos:origin="ec" xml:id="akn_article_ebFhaW_gX0mG3">
										<content leos:origin="ec" xml:id="akn_article_ebFhaW_xRZkW7">
											<p leos:origin="ec" xml:id="akn_article_ebFhaW_kSgpRW">Third point,</p>
										</content>
									</alinea>
									<list leos:origin="ec" xml:id="_lis_IUu0o5">
										<point leos:origin="ec" xml:id="akn_article_ebFhaW_HPtvHs">
											<num leos:origin="ec" xml:id="akn_article_ebFhaW_WP5mTp">(4)</num>
											<content leos:origin="ec" xml:id="akn_article_ebFhaW_ucGIvY">
												<p leos:origin="ec" xml:id="akn_article_ebFhaW_TOvjYr">inward processing;</p>
											</content>
										</point>
									</list>
								</point>
								<point leos:origin="cn" leos:softaction="add" leos:softdate="2021-12-09T15:49:38.693+01:00" leos:softuser="jane" xml:id="_akn_point_Z987tG">
									<num leos:editable="false" leos:origin="cn" xml:id="_akn_article_ebFhaW_nNiC7G">(-iiia)</num>
									<content xml:id="_akn_article_ebFhaW_3cTKKl">
										<p xml:id="_akn_article_ebFhaW_7s9OEH">Text...</p>
									</content>
								</point>
								<point leos:origin="ec" xml:id="akn_article_ebFhaW_zpT8yJ">
									<num leos:origin="ec" xml:id="akn_article_ebFhaW_TE2moF">(iii)</num>
									<alinea leos:origin="ec" xml:id="akn_article_ebFhaW_vocLxG">
										<content leos:origin="ec" xml:id="akn_article_ebFhaW_7CY6ew">
											<p leos:origin="ec" xml:id="akn_article_ebFhaW_Bsi83y">'holder of the goods' means holder of the goods as defined in point (34) of Article 5 of Regulation (EU) No 952/2013;</p>
										</content>
									</alinea>
									<alinea leos:origin="ec" xml:id="akn_article_ebFhaW_UEnmZd">
										<content leos:origin="ec" xml:id="akn_article_ebFhaW_hW1d9s">
											<p leos:origin="ec" xml:id="akn_article_ebFhaW_84Z8LX">This is a subpoint</p>
										</content>
									</alinea>
								</point>
							</list>
						</point>
						<point leos:origin="ec" xml:id="akn_article_ebFhaW_vH1BEF">
							<num leos:origin="ec" xml:id="akn_article_ebFhaW_GnJiQJ">(d)</num>
							<alinea leos:origin="ec" xml:id="akn_article_ebFhaW_s5UCAV">
								<content leos:origin="ec" xml:id="akn_article_ebFhaW_0au81f">
									<p leos:origin="ec" xml:id="akn_article_ebFhaW_IiZufh">Ut non mattis est. Cras laoreet erat ut congue sollicitudin. Integer semper tellus in enim porta eleifend. Integer porta eleifend auctor.</p>
								</content>
							</alinea>
							<alinea leos:origin="ec" xml:id="akn_article_ebFhaW_Cljeg1">
								<content leos:origin="ec" xml:id="akn_article_ebFhaW_9BVg2t">
									<p leos:origin="ec" xml:id="akn_article_ebFhaW_CjZohW">Integer aliquam magna erat. Nam fermentum ipsum in dui pellentesque sollicitudin. Morbi scelerisque enim vel pulvinar lobortis. Suspendisse maximus lobortis vestibulum.</p>
								</content>
							</alinea>
