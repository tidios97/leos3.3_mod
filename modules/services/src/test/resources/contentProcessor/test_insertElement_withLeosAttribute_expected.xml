        <paragraph xml:id="art485-par1">
            <num class="Paragraph(numbered)" xml:id="n2">1.</num>
            <content xml:id="c1">
                <p class="Paragraph(numbered)" xml:id="p1">Subject to paragraph 2, this Regulation
                    <authorialNote marker="2" xml:id="a1">
                        <p xml:id="ptest1">TestNote1</p>
                    </authorialNote>
                    shall apply from 1 January 2013.
                </p>
            </content>
        </paragraph>
        <paragraph xml:id="art485-par2">
            <num class="Paragraph(numbered)" xml:id="n2">2.</num>
            <content xml:id="con">
                <p class="Paragraph(numbered)" xml:id="p2">Article 436(1) shall apply from 1 January 2015
                    <authorialNote marker="4" xml:id="a2">
                        <p xml:id="ptest2">TestNote2</p>
                    </authorialNote>
                    .
                </p>
            </content>
        </paragraph>
    </article>
